server {
    listen 80;
    server_name logotype.lvdsgn.com;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /root/pdf_create;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/root/pdf_create/pdfcreate/pdfcreate.sock;
    }
}